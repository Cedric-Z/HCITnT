<!DOCTYPE html>
<html lang="en">

<head>

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">
    <%- include('public/head'); %>

    <title>Creative - Start Bootstrap Theme</title>


</head>

<body id="page-top">

<%- include('public/navbar'); %>
<header class="masthead text-center text-white d-flex">
    <div class="container my-auto tnt-container">
        <!--<div class="row">-->
        <!--<div class="col-lg-10 mx-auto">-->
        <!--<h1 class="text-uppercase">-->
        <!--<strong>Fuck You!</strong>-->
        <!--</h1>-->
        <!--</div>-->
        <!--</div>-->
        <div class="row search-three-steps-nav">
            <div class="col-lg-4">
                <div class="step-wrapper step-now" id="search_what_button">
                    <span class="step-icon step-icon-now">
                        <i class="fa fa-question-circle" aria-hidden="true"></i>
                    </span>
                    <a class="step-name step-name-now" href="/register-tutor" style="color: white">Select Subject</a>
                </div>
            </div>
            <div class="col-lg-4">
                <div class="step-wrapper step-now" id="search_when_button">
                    <span class="step-icon step-icon-now">
                        <i class="fa fa-question-circle" aria-hidden="true"></i>
                    </span>
                    <span class="step-name step-name-now">Basic Information</span>
                </div>
            </div>
            <div class="col-lg-4">
                <div class="step-wrapper step-next" id="search_where_button">
                    <span class="step-icon step-icon-next">
                        <i class="fa fa-question-circle" aria-hidden="true"></i>
                    </span>
                    <span class="step-name step-name-now">Personal Profile</span>
                </div>
            </div>
        </div>
        <div class="content-agileits">
            <form action="#" method="post" data-toggle="validator" role="form">
                <div class="form-group  w3 w3l">
                    <label for="firstname" class="control-label">First Name</label>
                    <input type="text" class="form-control" id="firstname" placeholder="First Name" data-error="Enter Your First Name" required>
                    <div class="help-block with-errors"></div>
                </div>
                <div class="form-group agileits">
                    <label for="lastname" class="control-label">Last Name</label>
                    <input type="text" class="form-control" id="lastname" placeholder="Last Name" data-error="Enter Your Last Name" required>
                    <div class="help-block with-errors"></div>
                </div>
                <div class="form-group agileinfo wthree  agile">
                    <label for="Phone" class="control-label">Gender</label>
                    <select class="selectpicker form-control bg-transparent" style="color: white">
                        <option style="color: darkgrey">Male</option>
                        <option style="color: darkgrey">Female</option>
                    </select>
                </div>
                <div class="form-group agileits">
                    <label for="lastname" class="control-label">Date of Birth</label>
                    <input type="text" class="form-control" id="lastname" placeholder="MM-DD-YYYY" data-error="Enter Your Birth" required>
                    <div class="help-block with-errors"></div>
                </div>
                <div class="form-group w3l agileinfo wthree">
                    <label for="inputEmail" class="control-label">Email (with edu)</label>
                    <input type="email" class="form-control" id="inputEmail" placeholder="Email" data-error="This email address is invalid" required>
                    <div class="help-block with-errors"></div>
                </div>
                <div class="form-group agileits">
                    <label for="lastname" class="control-label">College</label>
                    <input type="text" class="form-control" id="lastname" placeholder="College" data-error="Enter Your College" required>
                    <div class="help-block with-errors"></div>
                </div>
                <div class="form-group agileits">
                    <label for="lastname" class="control-label">Major</label>
                    <input type="text" class="form-control" id="lastname" placeholder="Major" data-error="Enter Your Major" required>
                    <div class="help-block with-errors"></div>
                </div>
                <div class="form-group agileits">
                    <label for="lastname" class="control-label">Address1</label>
                    <input type="text" class="form-control" id="lastname" placeholder="Address1" data-error="Enter Your Address" required>
                    <div class="help-block with-errors"></div>
                </div>
                <div class="form-group agileits">
                    <label for="lastname" class="control-label">Address2</label>
                    <input type="text" class="form-control" id="lastname" placeholder="Address2">
                </div>
                <div class="form-group agileits">
                    <label for="lastname" class="control-label">State</label>
                    <input type="text" class="form-control" id="lastname" placeholder="State" data-error="Enter Your State" required>
                </div>
                <div class="form-group agileits">
                    <label for="lastname" class="control-label">City</label>
                    <input type="text" class="form-control" id="lastname" placeholder="City" data-error="Enter Your City" required>
                </div>
                <div class="form-group agileinfo wthree agile">
                    <label for="Phone" class="control-label">Phone</label>
                    <input type="text" class="form-control" id="Phone" placeholder="Phone" data-error="Enter Your Phone Number" required>
                    <div class="help-block with-errors"></div>
                </div>
                <div class="form-group agileinfo wthree  agile">
                    <label for="Phone" class="control-label">Zip Code</label>
                    <input type="text" class="form-control" id="Phone" placeholder="123" data-error="Enter Your Phone Number" required>
                    <div class="help-block with-errors"></div>
                </div>
                <div class="form-group agileinfo wthree  agile">
                    <label for="Phone" class="control-label">Prefer Location</label>
                    <select class="selectpicker form-control bg-transparent" style="color: white">
                        <option style="color: darkgrey">On Campus</option>
                        <option style="color: darkgrey">In my place</option>
                        <option style="color: darkgrey">In student's place</option>
                    </select>
                </div>
                <div class="form-group agile">
                    <label for="inputPassword" class="control-label">Password</label>
                    <div class="form-inline row">
                        <div class="form-group col-sm-6 ">
                            <input type="password" data-minlength="6" class="form-control" id="inputPassword" placeholder="Password">
                        </div>
                        <div class="form-group col-sm-6 ">
                            <input type="password" class="form-control" id="inputPasswordConfirm" data-match="#inputPassword" data-match-error="Whoops, these don't match" placeholder="Confirm Password" >
                            <div class="help-block with-errors"></div>
                        </div>
                    </div>
                </div>
                <div class="form-group">
                    <button type="submit" class="btn btn-lg" id="submit">Submit</button>
                </div>
            </form>
        </div>
        <!--<div class="progress">-->
        <!--<div class="progress-bar progress-bar-striped progress-bar-animated" role="progressbar" style="width: 33%"-->
        <!--aria-valuenow="33" aria-valuemin="0" aria-valuemax="100"></div>-->
        <!--</div>-->
    </div>
    <script>
        $("#submit").click(function () {
            location.href="register-tutor-personal-profile";
        })
    </script>
</header>
</body>

</html>
